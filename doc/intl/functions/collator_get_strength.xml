<refentry xml:id="function.collator_get_strength" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>collator_get_strength()</refname>
  <refpurpose>Get current collation strength.</refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>integer</type><methodname>collator_get_strength</methodname>
   <methodparam><type>Collator</type><parameter>$coll</parameter></methodparam>
  </methodsynopsis>
  <para>
   <!-- description goes here -->
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>$coll</parameter></term>
     <listitem>
      <para>
       Collator object.
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>
   
 
 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Returns current collation strength, or <type>boolean</type> &false; on error.
  </para>
 </refsect1>
 
 <refsect1 role="examples">
  &reftitle.examples;
  <para>
   <example>
    <title><function>collator_get_strength</function> example</title>
    <programlisting role="php">
<![CDATA[
<?php
$coll = collator_create( 'en_US' );
$strength = collator_get_strength( $coll );
?>
]]>
    </programlisting>
    &example.outputs;
    <screen>
<![CDATA[
Strength value is TERTIARY
]]>
    </screen>
   </example>
  </para>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><function>collator_set_strength</function></member>
    <member><function>collator-getStrength</function></member>
   </simplelist>
  </para>
 </refsect1>
</refentry>

