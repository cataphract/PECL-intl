<refentry xml:id="function.collator-getErrorCode" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>Collator->getErrorCode</refname>
  <refpurpose>Get collator's object last error code.</refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>integer</type><methodname>Collator->getErrorCode</methodname>
   <void />
  </methodsynopsis>
  <para>
  <!-- description goes here -->
  </para>
 </refsect1>

 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Error code returned by the last Collator method call.
  </para>
 </refsect1>
 
 <refsect1 role="examples">
  &reftitle.examples;
  <para>
   <example>
    <title><function>Collator->getErrorCode</function> example</title>
    <programlisting role="php">
<![CDATA[
<?php
$coll = new Collator( 'en_US' );
if( $coll->getAttribute( Collator::FRENCH_COLLATION ) === false )
    handle_error( $coll->getErrorCode() );
?>
]]>
    </programlisting>
   </example>
  </para>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><function>Collator->getErrorMessage</function></member>
    <member><function>collator_get_error_code</function></member>
    <member><function>intl_get_error_code</function></member>
   </simplelist>
  </para>
 </refsect1>
</refentry>

