<refentry xml:id="function.collator_get_display_name" xmlns="http://docbook.org/ns/docbook">
 <refnamediv>
  <refname>collator_get_display_name()</refname>
  <refpurpose>Get the display name for a locale.</refpurpose>
 </refnamediv>
 <refsect1 role="description">
  &reftitle.description;
  <methodsynopsis>
   <type>string</type><methodname>collator_get_display_name</methodname>
   <methodparam><type>string</type><parameter>obj_loc</parameter></methodparam>
   <methodparam><type>string</type><parameter>disp_loc</parameter></methodparam>
  </methodsynopsis>
  <para>
   The display name is suitable for presentation to a user.
  </para>
 </refsect1>

 <refsect1 role="parameters">
  &reftitle.parameters;
  <para>
   <variablelist>
    <varlistentry>
     <term><parameter>obj_loc</parameter></term>
     <listitem>
      <para>
       Locale to get display name for.  
      </para>
     </listitem>
    </varlistentry>
    <varlistentry>
     <term><parameter>disp_loc</parameter></term>
     <listitem>
      <para>
       Locale for display.
      </para>
     </listitem>
    </varlistentry>
   </variablelist>
  </para>
 </refsect1>
   
 
 <refsect1 role="returnvalues">
  &reftitle.returnvalues;
  <para>
   Returns locale name, or <type>boolean</type> &false; on error.
  </para>
 </refsect1>
 
 <refsect1 role="examples">
  &reftitle.examples;
  <para>
   <example>
    <title><function>collator_get_display_name</function> example</title>
    <programlisting role="php">
<![CDATA[
<?php
$text = collator_get_display_name( 'de_DE', 'en_US' );
echo $text . "\n";
?>
]]>
    </programlisting>
    &example.outputs;
    <screen>
German (Germany)
    </screen>
   </example>
  </para>
 </refsect1>

 <refsect1 role="seealso">
  &reftitle.seealso;
  <para>
   <simplelist>
    <member><function>collator_get_locale</function></member>
    <member><function>collator_get_available_locales</function></member>
    <member><function>Collator->getDisplayName</function></member>
   </simplelist>
  </para>
 </refsect1>
</refentry>

